{
  "manifest_version": 2,

  "name": "RetroTimes - Nein danke!",
  "description": "Entfernt Anti-AdBlock Scripts auf RetroTimes.in",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "https://retrotimes.in/*",
    "http://retrotimes.in/*"
  ],
  "content_scripts": [
    {
        "matches": ["http://retrotimes.in/*", "https://retrotimes.in/*"],
        "js": ["rt-neindanke.js"],
        "run_at": "document_start"
    }
    ]
}